    <script>
      let sizeIs
      function showQuantity(size, id) {
        document.getElementById("sizeX").value=null
        sizeIs=size
        $.ajax({
          url: `/quantity?id=${id}&size=${size}`,
          type: "POST",
          contentType: "application/json",
          success: function (response) {
           var productMetaList = document.getElementById('productMeta');

        // Check if an <li> element with id "quantityPlace" already exists
        var existingListItem = document.getElementById('quantityPlace');

        // If it doesn't exist and the condition is true, create and append the new <li> element
        if (!existingListItem ) {
            var newListItem = document.createElement('li');
            newListItem.textContent = 'Availability : ';
            var spanElement = document.createElement('span');
            spanElement.id = 'quantityPlace';
            newListItem.appendChild(spanElement);
            productMetaList.appendChild(newListItem);
        }
            const quantity = document.getElementById("quantityPlace");
            if (response.message == 0) {
              quantity.innerText = "item out of stock";
              quantity.style.color = "red";
              document.getElementById("sizeX").value=parseInt(response.message)
              
            }else if(response.message<10){
              quantity.innerText = `Hurry up only ${response.message} items left`;
              quantity.style.color = "red";
              


            } 
            else {
              quantity.innerText = "Available"
              quantity.style.color = "green";
              
            }
            productCartCheck(id)

          },

          error: function (error) {
            console.log("ajaxerror", error);
          },
        });
      }
      
    </script>
