      function verifyPayment(payment,order,body){
        
        $.ajax({
        url: `/verify-payment`,
        method: 'POST',
        contentType: 'application/json',
        data: JSON.stringify({ payment: payment, order: order ,body:body}),
        success: function(response) {
           if (response.success) {
            console.log("verify function success keri")
            window.location.href = response.url;
           }else{
            Toastify({
                text: response.message,
                 backgroundColor: "red",
             duration: 6000,
             position: "center" 
         }).showToast();

           }


        },
        error: function(error) {
          console.log("error functionil vannu")
            
            window.location.href = '/login';
        }
    });


      }

