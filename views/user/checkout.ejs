   function applyCoupon(){
       const couponCode=document.getElementById("couponInput").value
       if(couponCode===''){
         Toastify({
                        text: ` Please enter a coupon code`,
                        backgroundColor: "red",
                        duration: 3000,
                        position: "center" 
                    }).showToast();
                    return


       }
       $.ajax({
        url:`/applyCoupon?code=${couponCode}`,
        type:"POST",
        success:function(response){
          if(response.success){
              Toastify({
                        text: `coupon applied`,
                        backgroundColor: "green",
                        duration: 3000,
                        position: "center" 
                    }).showToast();
                  document.getElementById("applybtn").classList.add("disabled");
            const productSubtotalTds = document.querySelectorAll(".product-subtotal");
    productSubtotalTds.forEach(td => {
        td.innerText = `₹ ${response.totalAmount}`; 
    });
    const subtotal=document.getElementById("subtotal")
    subtotal.innerText=`₹ ${response.totalAmount}`
            const appliedExist=document.getElementById("couponapplied")
            if(!appliedExist){
              const newRow = document.createElement("tr");
             newRow.id = "couponapplied"
                newRow.innerHTML = `
            <th>Coupon Applied</th>
            <td colspan="2">
                ${response.couponName}
                <button class="btn btn-sm btn-danger" onclick="clearCoupon('${response.couponId}')">X</button>
            </td>
        `
                // Insert the new row before the total row
                const totalRow = document.getElementById("totalRow");
                totalRow.parentNode.insertBefore(newRow, totalRow);
            }else{
              appliedExist.innerHTML = `
            <th>Coupon Applied</th>
            <td colspan="2">${response.couponName}</td>
        `;

            }


          }
          else{
             Toastify({
                        text: `${response.message}`,
                        backgroundColor: "red",
                        duration: 3000,
                        position: "center" 
                    }).showToast();

          }
          

        },

        error:function(error){
          console.log(error)
        }
       })

   }
