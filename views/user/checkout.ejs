      $(document).ready(function(){
        $("#placeOrder").click(function(){
          let address=$("input[name='address']:checked").val()
          let payment=$("input[name='payment_option']:checked").val()
          if(address==null){
             Toastify({
                        text: `please select a address`,
                        backgroundColor: "red",
                        duration: 3000,
                        position: "center" 
                    }).showToast();
                    return

          }
          const data={
            addressId:address,
            paymentOption:payment
          }
          
          $.ajax({
            url:"/placeorder",
            method:"POST",
            data:data,
            success:function(response){
              if(response.success){
                window.location.href=response.url
              }else if(response.name){
                Toastify({
                text: `The product named ${response.name} is out of stock or has shortage`,
                duration: 3000,
                gravity: "top",
                position: "center",
                stopOnFocus: true,
                style: {
                  background: "linear-gradient(to right, aqua, blue)",
                },
              }).showToast();

              }
              if(response.status){
                const order=response.order
                razorPayment(order,response.body)
              }
              if(response.done){
                window.location.href=response.url

              }else if(response.message){
                Toastify({
                text: `Add money to wallet`,
                duration: 3000,
                gravity: "top",
                position: "center",
                stopOnFocus: true,
                style: {
                  background: "linear-gradient(to right, aqua, blue)",
                },
              }).showToast();

              }

            }
          })
        })

      })
      function razorPayment(order,body){
        var options = {
    "key": "rzp_test_ju6rorodT9IZ3H", // Enter the Key ID generated from the Dashboard
    "amount": order.amount, // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
    "currency": "INR",
    "name": "Kalikaal", //your business name
    "description": "Test Transaction",
    "image": "https://example.com/your_logo",
    "order_id": order.id, //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
    "handler": function (response){
        verifyPayment(response,order,body)
    },
    "prefill": { //We recommend using the prefill parameter to auto-fill customer's contact information, especially their phone number
        "name": "Gaurav Kumar", //your customer's name
        "email": "gaurav.kumar@example.com", 
        "contact": "9000090000"  //Provide the customer's phone number for better conversion rates 
    },
    "notes": {
