  // Add event listener to the select element
document.getElementById("sortByDropdown").addEventListener("change", function() {
 
    var selectedOption = this.value;

    switch (selectedOption) {
       
        case "lowToHigh":
            document.getElementById("sortByPriceLowForm").submit();
            break;
        case "highToLow":
            document.getElementById("sortByPriceHighForm").submit();
            break;
        case "releaseDate":
            document.getElementById("sortByReleaseDateForm").submit();
            break;
        case "aAtozZ":
             document.getElementById("sortByaAtozZ").submit();
             break;
        case "zZtoaA":
             document.getElementById("sortByzZtoaA").submit();
             break;

        default:
            // Handle default case
            break;
    }
});

</script>
