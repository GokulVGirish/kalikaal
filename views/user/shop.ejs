                     <% if (locals.sortpage) { %>
                      <% if (totalPages) { %>
  <div class="pagination">
    <% if (currentPage > 1) { %>
      <form id="prevForm" action="<%= baseRoute %>?condi=<%= condition %>" method="POST" style="display: inline;">
        <input type="hidden" name="page" value="<%= currentPage - 1 %>">
        <input type="hidden" name="pagenationProduct"  value="<%=JSON.stringify(allproducts) %>">
        
        <button type="submit" class="btn btn-sm" style="color: white;">Previous</button>
      </form>
    <% } %>

    <% for (let i = 1; i <= totalPages; i++) { %> 
      <form id="page<%= i %>Form" action="<%= baseRoute %>?condi=<%= condition %>" method="POST" style="display: inline;">
        <input type="hidden" name="page" value="<%= i %>">
        <input type="hidden" name="pagenationProduct"  value="<%=JSON.stringify(allproducts) %>">
        <button type="submit" class="btn btn-sm <%= i === currentPage ? 'active' : '' %>" style="color: white;"><%= i %></button>
      </form>
    <% } %>

    <% if (currentPage < totalPages) { %>
      <form id="nextForm" action="<%= baseRoute %>?condi=<%= condition %>" method="POST" style="display: inline;">
        <input type="hidden" name="page" value="<%= currentPage + 1 %>">
        <input type="hidden" name="pagenationProduct"  value="<%=JSON.stringify(allproducts) %>">
        <button type="submit" class="btn btn-sm" style="color: white;">Next</button>
      </form>
    <% } %>
  </div>
<% } %>

                      
                     <% }else{ %>


                <% if (totalPages) { %>
                    <div class="pagination">
                      <% if (currentPage > 1) { %>
                        <a class="btn btn-sm" href="<%= baseRoute%>?page=<%= currentPage - 1 %>" style="color: white;">Previous</a>
                      <% } %>
                  
                      <% for (let i = 1; i <= totalPages; i++) { %> 
                        
                          <a class="btn btn-sm <%= i === currentPage ? 'active' : '' %>" href="<%= baseRoute%>?page=<%= i %>" style="color: white;" ><%= i %></a>
                        
                      <% } %>
                  
                      <% if (currentPage < totalPages) { %>
                        <a class="btn btn-sm" href="<%= baseRoute%>?page=<%= currentPage + 1 %>" style="color: white;">Next</a>
                      <% } %>
                    </div>
                  <% } %>
                  <% } %>
                  
